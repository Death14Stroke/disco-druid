<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_home"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.andruid.magic.discodruid.ui.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_home_to_track"
            app:destination="@id/nav_track" />
    </fragment>

    <fragment
        android:id="@+id/nav_track"
        android:name="com.andruid.magic.discodruid.ui.fragment.TrackFragment"
        android:label="TrackFragment" />
</navigation>